{
  "name": "@rccpr/cli",
  "version": "0.1.4",
  "description": "CLI project",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "bin": {
    "cli": "./cli"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "start": "tsup --onSuccess 'node dist/index.js'",
    "dev": "tsup --watch --onSuccess 'node dist/index.js'",
    "lint": "eslint . -c ./src/*.ts"
  },
  "author": {
    "name": "Payment CLI",
    "url": "https://www.paymentcli.xyz/"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "consola": "^3.2.3",
    "dotenv": "^16.4.5",
    "execa": "^9.5.1",
    "ora": "^8.1.1",
    "prompts": "^2.4.2",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
